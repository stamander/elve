
<%= form_for @shop,url:shop_index_path,local:true  do |f| %>

  お名前<%= f.text_field :name %><br>
  カナ<%= f.text_field :kana %><br>
  電話番号<%= f.text_field :phone_number %><br>
  メールアドレス<%= f.text_field :mail %><br>
  郵便番号<%= f.text_field :adress_number %><br>
  住所<%= f.text_field :adress %><br>


  <%= f.submit %>
<% end %>
